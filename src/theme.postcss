:root {
	/* =~= Theme Properties =~= */
	--theme-font-family-base: system-ui;
	--theme-font-family-heading: ui-monospace, SFMono-Regular, Menlo, Monaco, Consolas, "Liberation Mono", "Courier New", monospace;
	--theme-font-color-base: 0 0 0;
	--theme-font-color-dark: 255 255 255;
	--theme-rounded-base: 0px;
	--theme-rounded-container: 0px;
	--theme-border-base: 1px;
	/* =~= Theme On-X Colors =~= */
	--on-primary: 255 255 255;
	--on-secondary: 0 0 0;
	--on-tertiary: 255 255 255;
	--on-success: 0 0 0;
	--on-warning: 0 0 0;
	--on-error: 0 0 0;
	--on-surface: 0 0 0;
	/* =~= Theme Colors  =~= */
	/* primary | #0072e3 */
	--color-primary-50: 217 234 251; /* ⬅ #d9eafb */
	--color-primary-100: 204 227 249; /* ⬅ #cce3f9 */
	--color-primary-200: 191 220 248; /* ⬅ #bfdcf8 */
	--color-primary-300: 153 199 244; /* ⬅ #99c7f4 */
	--color-primary-400: 77 156 235; /* ⬅ #4d9ceb */
	--color-primary-500: 0 114 227; /* ⬅ #0072e3 */
	--color-primary-600: 0 103 204; /* ⬅ #0067cc */
	--color-primary-700: 0 86 170; /* ⬅ #0056aa */
	--color-primary-800: 0 68 136; /* ⬅ #004488 */
	--color-primary-900: 0 56 111; /* ⬅ #00386f */
	/* secondary | #ff6100 */
	--color-secondary-50: 255 231 217; /* ⬅ #ffe7d9 */
	--color-secondary-100: 255 223 204; /* ⬅ #ffdfcc */
	--color-secondary-200: 255 216 191; /* ⬅ #ffd8bf */
	--color-secondary-300: 255 192 153; /* ⬅ #ffc099 */
	--color-secondary-400: 255 144 77; /* ⬅ #ff904d */
	--color-secondary-500: 255 97 0; /* ⬅ #ff6100 */
