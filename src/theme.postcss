	--color-surface-900: 111 112 108; /* ⬅ #6f706c */
	
    --html: '#13ec96';
    --css: '#ec1369';
    --typescript: '#1f36e0';
    --javascript: '#f8d507';
    --fundamentals: '#780db3';
    --react: '#0daff2';
    --svelte: '#f2500d';
}
